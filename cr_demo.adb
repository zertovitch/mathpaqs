--  Demo for the ConRec contour plot routine.

with PDF_Out;
with Contours;

procedure CR_Demo is

  type My_Float is digits 15;

  type Function_Access is access function (x, y : My_Float) return My_Float;

  procedure Display_Contours
    (pdf            : in out PDF_Out.PDF_Out_File;
     f              : in    Function_Access;
     x1, y1, x2, y2 : in    PDF_Out.Real;
     nx             : in    Positive;   --  Number of x values - 1 (zero-based)
     ny             : in    Positive;   --  Number of y values - 1 (zero-based)
     nc             : in    Positive)   --  Number of contour levels (z-axis)
  is

    use PDF_Out;

    z_min : Real := Real'Last;
    z_max : Real := Real'First;

    procedure Draw_Segment (xa, ya, xb, yb, z : PDF_Out.Real) is
      z01 : constant PDF_Out.Real := (z - z_min) / (z_max - z_min);
    begin
      pdf.Stroking_Color ((z01, 0.0, 1.0 - z01));
      pdf.Single_Line ((xa, ya), (xb, yb));
    end Draw_Segment;

    package CRG is new Contours (PDF_Out.Real, Draw_Segment);

    d : CRG.Contour_Data  (0 .. nx, 0 .. ny);
    x : CRG.Contour_Pos   (d'Range (1));
    y : CRG.Contour_Pos   (d'Range (2));
    z : CRG.Contour_Level (1 .. nc);

  begin
    --
    --  Fill the d, x, y, z arrays.
    --

    for i in 0 .. nx loop
      x (i) := Real (i) * (x2 - x1) / Real (nx) + x1;
    end loop;

    for j in 0 .. ny loop
      y (j) := Real (j) * (y2 - y1) / Real (ny) + y1;
    end loop;

    for i in 0 .. nx loop
      for j in 0 .. ny loop
        d (i, j) := PDF_Out.Real (f (My_Float (x (i)), My_Float (y (j))));
        z_min := Real'Min (z_min, d (i, j));
        z_max := Real'Max (z_max, d (i, j));
      end loop;
    end loop;

    for i in 1 .. nc loop
      z (i) := Real (i) * (z_max - z_min) / Real (nc + 1);
    end loop;

    pdf.Set_Math_Plane ((x1, y1, x2 - x1, y2 - y1));

    --
    --     Draw a border around the contour plot
    --

    Draw_Segment (x1, y1, x1, y2, 0.0);
    Draw_Segment (x1, y2, x2, y2, 0.0);
    Draw_Segment (x2, y2, x2, y1, 0.0);
    Draw_Segment (x2, y1, x1, y1, 0.0);

    --
    --     Draw a grid (dots)
    --
    for i in 1 .. nx - 1 loop
      for j in 1 .. ny - 1 loop
        Draw_Segment (x (i), y (j), x (i), y (j), d (i, j));
      end loop;
    end loop;

    --
    --     Call the contouring routine
    --
    CRG.Con_Rec (d, x, y, z);

  end Display_Contours;

